<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="artistVM" type="com.musicplayer.app.modules.artist.data.viewmodel.ArtistVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_width="@dimen/_10px" android:src="@drawable/img_icon15"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtRenaissance"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_weight="0.54"
                    android:layout_width="0dp"
                    android:text="@{artistVM.artistModel.txtRenaissance}" style="@style/textStyleRobotoregular20"/>
                <ImageView android:id="@+id/image1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_16px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_width="@dimen/_19px" android:src="@drawable/img_icon16"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10px"
            android:paddingEnd="@dimen/_30px" android:paddingStart="@dimen/_30px">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleblack_900">
                <ImageView android:id="@+id/image2"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_38px"
                    android:layout_marginEnd="@dimen/_50px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_marginTop="@dimen/_100px"
                    android:layout_width="@dimen/_38px" android:src="@drawable/img_favorites3_1"/>
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_50px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_marginTop="@dimen/_8px"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtRenaissance1"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_4px"
                        android:layout_width="match_parent"
                        android:text="@{artistVM.artistModel.txtRenaissance1}" style="@style/textStyleRobotomedium28"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linear3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_50px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_marginTop="@dimen/_5px"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStyle">
                    <FrameLayout android:id="@+id/frame1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" style="@style/groupStyle">
                        <TextView android:gravity="top|start"
                            android:id="@+id/txt843Tracks"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_2px"
                            android:layout_width="match_parent"
                            android:text="@{artistVM.artistModel.txt843Tracks}" style="@style/textStyleRobotoregular16"/>
                    </FrameLayout>
                    <FrameLayout
                        android:background="@color/white_A700_95"
                        android:id="@+id/frame2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_3px"
                        android:layout_marginStart="@dimen/_4px" android:layout_width="@dimen/_3px"/>
                    <FrameLayout android:id="@+id/frame3"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5px"
                        android:layout_width="wrap_content" style="@style/groupStyle">
                        <TextView android:gravity="top|start"
                            android:id="@+id/txt23Albums"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_2px"
                            android:layout_width="match_parent"
                            android:text="@{artistVM.artistModel.txt23Albums}" style="@style/textStyleRobotoregular16"/>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear4"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_40px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <LinearLayout android:id="@+id/linear5"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout android:id="@+id/frame4"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.48"
                            android:layout_width="0dp" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtNewReleases"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_4px"
                                android:layout_width="match_parent"
                                android:text="@{artistVM.artistModel.txtNewReleases}" style="@style/textStyleRobotobold26"/>
                        </FrameLayout>
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtViewAll"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_50px"
                            android:layout_weight="0.52"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_6px"
                            android:paddingStart="@dimen/_10px"
                            android:paddingTop="@dimen/_3px"
                            android:text="@{artistVM.artistModel.txtViewAll}" style="@style/textStyleRobotoregular14_1"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear6"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_13px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_yellow_700_radius_12"
                        android:id="@+id/frame5"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_181px" android:layout_width="@dimen/_159px"/>
                    <LinearLayout android:id="@+id/linear7"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3px"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyle">
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtUrgentSiege"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@{artistVM.artistModel.txtUrgentSiege}" style="@style/textStyleRobotomedium18"/>
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtDamnedAnthem"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5px"
                            android:layout_width="match_parent"
                            android:text="@{artistVM.artistModel.txtDamnedAnthem}" style="@style/textStyleRobotoregular14"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>

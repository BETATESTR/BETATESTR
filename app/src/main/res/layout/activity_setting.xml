<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingVM" type="com.musicplayer.app.modules.setting.data.viewmodel.SettingVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_49px"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_width="@dimen/_10px" android:src="@drawable/img_icon15"/>
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_weight="0.47"
                    android:layout_width="0dp" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtEqualizer"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_3px"
                        android:layout_width="match_parent"
                        android:text="@{settingVM.settingModel.txtEqualizer}" style="@style/textStyleRobotoregular20"/>
                </FrameLayout>
                <ImageView android:id="@+id/image1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_13px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_width="@dimen/_13px" android:src="@drawable/img_icon7"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollView"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10px"
            android:paddingEnd="@dimen/_30px" android:paddingStart="@dimen/_30px">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleblack_900">
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_37px"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_21px}"
                    style="@style/groupStyle" tools:itemCount="6" tools:listitem="@layout/row_setting1"/>
                <FrameLayout android:id="@+id/frame1"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_30px"
                    android:layout_marginTop="@dimen/_36px"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <LinearLayout android:id="@+id/linear3"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyle">
                        <FrameLayout android:id="@+id/frame2"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtMySettings"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_1px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtMySettings}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame3"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_30px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtDefault"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtDefault}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame4"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_30px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtClubMusic"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtClubMusic}" style="@style/textStyleRobotoregular16_2"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame5"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_30px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtClassicalMusic"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtClassicalMusic}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame6"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_30px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtDanceMusic"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtDanceMusic}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame7"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_30px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtLowFrequencyG"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtLowFrequencyG}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame8"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_30px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtLowFrequencyG1"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{settingVM.settingModel.txtLowFrequencyG1}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>

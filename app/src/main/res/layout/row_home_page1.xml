<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homePage1RowModel" type="com.musicplayer.app.modules.homepage.data.model.HomePage1RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStyle">
        <FrameLayout
            android:background="@drawable/rectangle_bg_yellow_700_radius_12"
            android:id="@+id/frame"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_181px" android:layout_width="@dimen/_159px"/>
        <TextView android:gravity="top|start"
            android:id="@+id/txtUrgentSiege"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3px"
            android:layout_width="wrap_content"
            android:text="@{homePage1RowModel.txtUrgentSiege}" style="@style/textStyleRobotomedium18"/>
        <TextView android:gravity="top|start"
            android:id="@+id/txtDamnedAnthem"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5px"
            android:layout_width="wrap_content"
            android:text="@{homePage1RowModel.txtDamnedAnthem}" style="@style/textStyleRobotoregular14"/>
    </LinearLayout>
</layout>

<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileVM" type="com.musicplayer.app.modules.profile.data.viewmodel.ProfileVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_49px"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_width="@dimen/_10px" android:src="@drawable/img_icon15"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtProfile"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_weight="0.41"
                    android:layout_width="0dp"
                    android:text="@{profileVM.profileModel.txtProfile}" style="@style/textStyleRobotoregular20"/>
                <ImageView android:id="@+id/image1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_13px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_width="@dimen/_13px" android:src="@drawable/img_icon7"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent" android:paddingBottom="@dimen/_10px">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleblack_900">
                <LinearLayout android:id="@+id/linear3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_50px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_marginTop="@dimen/_41px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <FrameLayout
                        android:background="@color/deep_orange_100"
                        android:id="@+id/frame"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_97px"
                        android:layout_marginEnd="@dimen/_16px"
                        android:layout_marginStart="@dimen/_16px" android:layout_width="@dimen/_97px"/>
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtErlikBachman"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_7px"
                        android:layout_marginStart="@dimen/_7px"
                        android:layout_marginTop="@dimen/_10px"
                        android:layout_width="match_parent"
                        android:text="@{profileVM.profileModel.txtErlikBachman}" style="@style/textStyleRobotomedium18"/>
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtBachmanMailCo"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_7px"
                        android:layout_width="match_parent"
                        android:text="@{profileVM.profileModel.txtBachmanMailCo}" style="@style/textStyleRobotoregular14"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frame1"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12px"
                    android:layout_marginStart="@dimen/_12px"
                    android:layout_marginTop="@dimen/_50px"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <LinearLayout android:id="@+id/linear4"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_19px"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_18px"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyleblack_900_9fcornerRadius">
                        <FrameLayout android:id="@+id/frame2"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17px"
                            android:layout_marginStart="@dimen/_17px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|center"
                                android:id="@+id/txtMySubscription"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2px"
                                android:layout_width="match_parent"
                                android:text="@{profileVM.profileModel.txtMySubscription}" style="@style/textStyleRobotoregular16"/>
                        </FrameLayout>
                        <FrameLayout android:id="@+id/frame3"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4px"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <TextView android:gravity="top|center"
                                android:id="@+id/txtValidUntilMay"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_3px"
                                android:layout_width="match_parent"
                                android:text="@{profileVM.profileModel.txtValidUntilMay}" style="@style/textStyleRobotoregular14"/>
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>
                <TextView android:gravity="top|center"
                    android:id="@+id/txtRestorePurchas"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12px"
                    android:layout_marginStart="@dimen/_12px"
                    android:layout_marginTop="@dimen/_27px"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20px"
                    android:paddingEnd="@dimen/_30px"
                    android:paddingStart="@dimen/_30px"
                    android:paddingTop="@dimen/_17px"
                    android:text="@{profileVM.profileModel.txtRestorePurchas}" style="@style/textStyleRobotoregular16_1"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:gravity="center"
                    android:id="@+id/btnEnterPromoCod"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12px"
                    android:layout_marginStart="@dimen/_12px"
                    android:layout_marginTop="@dimen/_8px"
                    android:layout_width="match_parent"
                    android:text="@string/msg_enter_promo_cod"
                    android:textAllCaps="false" style="@style/textStyleRobotoregular16_1"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:gravity="center" android:id="@+id/btnQuit"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12px"
                    android:layout_marginStart="@dimen/_12px"
                    android:layout_marginTop="@dimen/_8px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_quit"
                    android:textAllCaps="false" style="@style/textStyleRobotoregular16_1"/>
                <FrameLayout android:id="@+id/frame4"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_84px"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <LinearLayout android:id="@+id/linear5"
                        android:layout_gravity="center|start"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_8px"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyleblack_900_a6">
                        <LinearLayout android:id="@+id/linear6"
                            android:layout_gravity="end"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/image2"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_10px"
                                android:layout_width="@dimen/_18px" android:src="@drawable/img_icon13"/>
                            <LinearLayout android:id="@+id/linear7"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_50px"
                                android:layout_weight="0.57"
                                android:layout_width="0dp"
                                android:orientation="vertical" style="@style/groupStyle">
                                <FrameLayout android:id="@+id/frame5"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent" style="@style/groupStyle">
                                    <TextView
                                    android:gravity="top|start"
                                    android:id="@+id/txtUrgentSiege"
                                    android:layout_gravity="center"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/_2px"
                                    android:layout_width="match_parent"
                                    android:text="@{profileVM.profileModel.txtUrgentSiege}" style="@style/textStyleRobotomedium15"/>
                                </FrameLayout>
                                <FrameLayout android:id="@+id/frame6"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_2px"
                                    android:layout_marginStart="@dimen/_2px"
                                    android:layout_width="match_parent" style="@style/groupStyle">
                                    <TextView
                                    android:gravity="top|start"
                                    android:id="@+id/txtDamnedAnthem"
                                    android:layout_gravity="center"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/_2px"
                                    android:layout_width="match_parent"
                                    android:text="@{profileVM.profileModel.txtDamnedAnthem}" style="@style/textStyleRobotoregular11"/>
                                </FrameLayout>
                            </LinearLayout>
                            <ImageView android:id="@+id/image3"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_17px"
                                android:layout_marginStart="@dimen/_50px"
                                android:layout_width="@dimen/_16px" android:src="@drawable/img_icon14"/>
                        </LinearLayout>
                        <View android:background="@color/white_A700"
                            android:id="@+id/line"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_3px"
                            android:layout_marginTop="@dimen/_5px" android:layout_width="match_parent"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>

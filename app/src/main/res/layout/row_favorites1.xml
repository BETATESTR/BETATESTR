<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="favorites1RowModel" type="com.musicplayer.app.modules.favorites.data.model.Favorites1RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frame"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" style="@style/groupStyle">
        <FrameLayout android:id="@+id/frame1"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700_67cornerRadius">
            <LinearLayout android:id="@+id/linear"
                android:layout_gravity="center|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15px"
                android:layout_marginStart="@dimen/_18px"
                android:layout_marginTop="@dimen/_17px"
                android:layout_width="wrap_content"
                android:orientation="horizontal" style="@style/groupStyle">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24px"
                    android:layout_width="@dimen/_21px" android:src="@drawable/img_user"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtArtist"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_width="wrap_content"
                    android:text="@{favorites1RowModel.txtArtist}" style="@style/textStyleRobotoregular16"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
